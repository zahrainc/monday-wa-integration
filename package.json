{
  "name": "monday-integration-quickstart",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "start": "node ./src/app.js",
    "dev": "npm run stop && concurrently \"npm run server\" \"npm run expose\"",
    "server": "nodemon ./src/app.js",
    "expose": "mapps tunnel:create -p 8302",
    "stop": "kill-port 8302"
  },
  "nodemonConfig": {
    "watch": [
      ".env",
      "src/"
    ]
  },
  "dependencies": {
    "axios": "^1.7.2",
    "body-parser": "^1.20.2",
    "cross-port-killer": "^1.2.1",
    "dotenv": "^5.0.1",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "monday-sdk-js": "^0.5.5",
    "mongoose": "^8.5.2",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "@mondaycom/apps-cli": "^2.1.1",
    "concurrently": "^8.2.2",
    "debug": "~2.6.9",
    "eslint": "^v6.8.0",
    "nodemon": "^3.0.2"
  }
}
